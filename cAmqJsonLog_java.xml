<!DOCTYPE document [
<!ENTITY languages SYSTEM "../common/languages.xml">
<!ENTITY listnamespaces SYSTEM "../common/listnamespaces.xml">
]>
<COMPONENT>

	<HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

	COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20100401A"
    STARTABLE="false"
	SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>Miscellaneous</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ROUTE" MAX_INPUT="1" MAX_OUTPUT="1"/>  
		<CONNECTOR CTYPE="ROUTE_WHEN" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_OTHER" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_TRY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_CATCH" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_FINALLY" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ROUTE_ENDBLOCK" MAX_INPUT="1" MAX_OUTPUT="0" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="IMPORT" FIELD="MEMO_IMPORT" REQUIRED="false" NUM_ROW="05" NB_LINES="2">
			<DEFAULT>import org.apache.camel.model.dataformat.JsonLibrary;</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TYPE" FIELD="CLOSED_LIST" NUM_ROW="06"
			REQUIRED="true" READONLY="false" GROUP="URI_GROUP">
			<ITEMS DEFAULT="QUEUE">
				<ITEM NAME="QUEUE" VALUE="queue" />
				<ITEM NAME="TOPIC" VALUE="topic" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="DESTINATION" READONLY="false" FIELD="TEXT"
			REQUIRED="true" NUM_ROW="07" GROUP="URI_GROUP">
			<DEFAULT>"queue.name"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION_FACOTRY_CONFIGURATION" GROUP="URI_GROUP" FIELD="ROUTE_COMPONENT_TYPE" FILTER="cMQConnectionFactory"
			NUM_ROW="08">
		</PARAMETER>
<!--		
		<PARAMETER NAME="HOSTNAME" FIELD="TEXT" NUM_ROW="09">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="10">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="OPERATION" FIELD="TEXT" NUM_ROW="11">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CLUSTER" FIELD="TEXT" NUM_ROW="12">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="INDEXNAME" FIELD="TEXT" NUM_ROW="13">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="INDEXTYPE" FIELD="TEXT" NUM_ROW="14">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
-->		
		<PARAMETER NAME="VALUES" FIELD="TABLE" REQUIRED="true" 
			NUM_ROW="18" NB_LINES="6">
			<ITEMS>
				<ITEM NAME="NAME" VALUE="&quot;ObjectName&quot;" />	
				<ITEM NAME="LANGUAGE" FIELD="CLOSED_LIST">	
					<ITEMS DEFAULT="CONSTANT">
						&languages;
					</ITEMS>
				</ITEM>
				<ITEM NAME="EXPRESSION" VALUE="&quot;ObjectValue&quot;" READONLY_IF="VALUES.LANGUAGE == 'CORRELATION'"/>	
			</ITEMS>
		</PARAMETER>
    	&listnamespaces;
		<PARAMETER NAME="USE_LOGEIP" FIELD="RADIO" NUM_ROW="19"  >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NONE" FIELD="RADIO" NUM_ROW="20"  >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MESSAGE" FIELD="TEXT" NUM_ROW="21"  SHOW_IF="USE_LOGEIP=='true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT MODULE_GROUP="camel-groovy" REQUIRED_IF="VALUES.LANGUAGE IN ['groovy']" />
			<IMPORT MODULE_GROUP="camel-jsonpath-alldep" REQUIRED_IF="VALUES.LANGUAGE IN ['jsonpath']" />
			<IMPORT MODULE_GROUP="camel-saxon-alldep" REQUIRED_IF="VALUES.LANGUAGE IN ['xquery']" />
			<IMPORT MODULE_GROUP="camel-jackson-alldep"/>
			<!-- camel-script -->
			<IMPORT NAME="camel-script"
                    MODULE="camel-script-2.17.3.jar" MVN="mvn:org.apache.camel/camel-script/2.17.3"
                    UrlPath="platform:/plugin/org.talend.designer.camel.components.localprovider/lib/camel-script-2.17.3.jar"
                    REQUIRED_IF="VALUES.LANGUAGE IN ['javaScript']" />
					
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
